<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        tools:ignore="MissingConstraints">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/blueSky"
            android:textAlignment="center"
            app:navigationIcon="@drawable/ic_baseline_close_24"
            app:title="@string/payment_hasil"
            app:titleTextColor="@color/white" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:background="@color/blueSky"
        android:gravity="center_horizontal"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints"
        android:layout_height="0dp">

        <ImageView
            android:layout_marginTop="@dimen/_5sdp"
            android:layout_width="@dimen/_110sdp"
            android:layout_height="@dimen/_110sdp"
            android:background="@drawable/wrongicon"
            android:textAlignment="center"
            android:contentDescription="@string/gagal" />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/gagal"
            android:textColor="@color/red"
            android:textAlignment="center"
            android:fontFamily="@font/robotocondensed_regular"
            android:textSize="@dimen/_20ssp"/>
        <LinearLayout
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_width="@dimen/_290sdp"
            android:layout_height="@dimen/_355sdp"
            android:orientation="vertical"
            android:background="@drawable/background">
            <TextView
                android:layout_marginTop="@dimen/_20sdp"
                android:layout_marginStart="@dimen/_20sdp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/detail"
                android:textColor="@color/black"
                android:textSize="@dimen/_17ssp"
                android:fontFamily="@font/robotocondensed_regular"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_marginStart="@dimen/_20sdp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/item"
                    android:textSize="@dimen/_17ssp"
                    android:fontFamily="@font/robotocondensed_regular"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_100sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:fontFamily="@font/robotocondensed_regular"
                    android:text="@string/harga"
                    android:textSize="@dimen/_17ssp" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:layout_marginStart="@dimen/_20sdp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/item"
                    android:textSize="@dimen/_17ssp"
                    android:fontFamily="@font/robotocondensed_regular"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_100sdp"
                    android:fontFamily="@font/robotocondensed_regular"
                    android:text="@string/harga"
                    android:textSize="@dimen/_17ssp" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:layout_marginStart="@dimen/_20sdp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/item"
                    android:textSize="@dimen/_17ssp"
                    android:fontFamily="@font/robotocondensed_regular"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_100sdp"
                    android:fontFamily="@font/robotocondensed_regular"
                    android:text="@string/harga"
                    android:textSize="@dimen/_17ssp" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:layout_marginStart="@dimen/_20sdp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/item"
                    android:textSize="@dimen/_17ssp"
                    android:fontFamily="@font/robotocondensed_regular"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_100sdp"
                    android:fontFamily="@font/robotocondensed_regular"
                    android:text="@string/harga"
                    android:textSize="@dimen/_17ssp" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:layout_marginStart="@dimen/_20sdp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/item"
                    android:textSize="@dimen/_17ssp"
                    android:fontFamily="@font/robotocondensed_regular"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_100sdp"
                    android:fontFamily="@font/robotocondensed_regular"
                    android:text="@string/harga"
                    android:textSize="@dimen/_17ssp" />
            </LinearLayout>

            <TextView
                android:layout_marginTop="@dimen/_95sdp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:fontFamily="@font/robotocondensed_regular"
                android:text="@string/kembali"
                android:textColor="@color/blueSea"
                android:textSize="@dimen/_15ssp" />
            <com.google.android.material.button.MaterialButton
                android:id="@+id/bt_kembali"
                app:cornerRadius="@dimen/_5sdp"
                android:layout_width="@dimen/_250sdp"
                android:layout_height="@dimen/_45sdp"
                android:layout_gravity="center"
                android:backgroundTint="@color/blueSea"
                android:fontFamily="@font/robotocondensed_regular"
                android:shadowColor="@color/black"
                android:text="@string/menu_home"
                android:textAlignment="center"
                android:textAllCaps="false"
                android:textSize="@dimen/_14ssp"
                tools:ignore="MissingConstraints" />
        </LinearLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>